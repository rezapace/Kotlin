gb(161, 161, 170); }

.notification-widget-item__summary-icon { width: 1rem; height: 1rem; margin=
-right: 0.25rem; }

.notification-widget-item:not(:first-child) .notification-widget-item__meta=
::before, .notification-widget-item:not(:last-child) .notification-widget-i=
tem__summary::after { content: ""; display: block; position: absolute; widt=
h: 100%; height: 1px; left: 0px; border-top: 1px solid rgb(228, 228, 231); =
}

.notification-widget-item__meta::before { top: -17px; }

.notification-widget-item__summary::after { bottom: -17px; }

.notification-widget-item__actions { margin-top: 1rem; opacity: 0; }

.notification-widget-item:hover .notification-widget-item__actions { opacit=
y: 1; }

